{
  "name": "tool_box",
  "version": "1.0.1",
  "description": "Box for Tools",
  "repository": {
    "url": "git://github.com/iseeeva/tool_box.git"
  },
  "main": "dist/index.js",
  "files": [
    "README.md",
    "dist",
    "package.json"
  ],
  "scripts": {
    "start": "node dist/index.js",
    "start:dev": "tsx src/index.ts",
    "ts:build": "tsc && tsc-alias",
    "ts:watch": "tsc -w",
    "ts:check": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint --fix src/**/*.ts"
  },
  "dependencies": {
    "@twisine/r_lib": "1.0.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "2.21.3",
    "@types/node": "20.11.5",
    "eslint": "8.57.0",
    "tsc-alias": "^1.8.10",
    "tsx": "4.7.0",
    "typescript": "5.5.2"
  }
}
